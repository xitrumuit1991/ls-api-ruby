                  </tbody>
                </table>
                <div class="row">
                  <div class="pull-right">
                    <div class="col-xs-12">
                      <%= paginate @ios %>
                    </div>
                  </div>
                </div>
              </div>
              <div class="tab-pane <%= 'active' if params[:tab] == 'tab5' %>" id="tab5">
                <form action="" method="get">
                  <div class="pull-left">
                    <div class="col-xs-12">
