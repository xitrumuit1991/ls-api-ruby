                    </div>
                  </div>
                </form>
                <div class="clearfix"></div>
                <table class="table table-hover demo-table-search">
                  <thead>
                    <tr>
                      <th style="width:20%">Ngân hàng</th>
                      <th style="width:20%">Mệnh giá</th>
                      <th style="width:20%">Xu</th>
                      <th style="width:20%">Mô tả</th>
                      <th style="width:20%">Ngày</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @banks.each do |item| %>
                      <tr id="row_page_<%= item.id %>">
