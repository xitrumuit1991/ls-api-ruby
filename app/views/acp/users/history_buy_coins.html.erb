                    </tr>
                  </thead>
                  <tbody>
                    <% @ios.each do |item| %>
                      <tr id="row_page_<%= item.id %>">
                        <td class="v-align-middle ">
                          <p><%= item.name %></p>
                        </td>
                        <td class="v-align-middle ">
                          <p><%= item.price %></p>
                        </td>
                        <td class="v-align-middle ">
                          <p><%= item.price_usd %></p>
