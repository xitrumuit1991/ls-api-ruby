                  </div>
                </form>
                <div class="clearfix"></div>
                <table class="table table-hover demo-table-search">
                  <thead>
                    <tr>
                      <th style="width:20%">Gói</th>
                      <th style="width:20%">Giá VND</th>
                      <th style="width:20%">Giá USD</th>
                      <th style="width:20%">Xu</th>
                      <th style="width:20%">Ngày</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @android.each do |item| %>
                      <tr id="row_page_<%= item.id %>">
                        <td class="v-align-middle ">
                          <p><%= item.name %></p>
                        </td>
                        <td class="v-align-middle ">
                          <p><%= item.price %></p>
                        </td>
                        <td class="v-align-middle ">
                          <p><%= item.price_usd %></p>
                        </td>
                        <td class="v-align-middle ">
                          <p><%= item.quantity %></p>
                        </td>
                        <td class="v-align-middle">
                          <p><%= item.created_at.strftime('%d-%m-%Y') %></p>
                        </td>
                      </tr>
                    <% end %>
                  </tbody>
                </table>
                <div class="row">
                  <div class="pull-right">
                    <div class="col-xs-12">
                      <%= paginate @android %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
    </div>
    <!-- END PANEL -->
</div>